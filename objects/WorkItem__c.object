    <validationRules>
        <fullName>Done_cant_be_change</fullName>
        <active>false</active>
        <errorConditionFormula>IF(PRIORVALUE(Status__c) = &quot;In Progress&quot; &amp;&amp;  ISCHANGED(Status__c), ISPICKVAL(Status__c, &quot;Not Started&quot;), IF(PRIORVALUE(Status__c) = &quot;Done&quot;, ISPICKVAL(Status__c, &quot;Not Started&quot;) || ISPICKVAL(Status__c, &quot;In Progress&quot;), PRIORVALUE(Status__c) = &quot;Done&quot;))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>The order goes: Not Started -&gt; In Progress -&gt; Done.</errorMessage>
    </validationRules>
    <validationRules>
